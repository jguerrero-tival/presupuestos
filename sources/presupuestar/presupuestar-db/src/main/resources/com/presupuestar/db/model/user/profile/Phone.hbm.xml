<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.presupuestar.model.user.profile.Phone" schema="PRESUPUESTAR"
		table="PHONE" entity-name="Phone">

		<id name="id" type="long" column="PHONE_ID">
			<generator class="native" />
		</id>

		<property name="phoneNumber" type="string" column="PHONE_NUMBER"
			length="20" not-null="true" />

	</class>
</hibernate-mapping>