<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.presupuestar.model.budget.LaborDetail"
		schema="PRESUPUESTAR" table="LABOR_DETAIL" entity-name="LaborDetail">

		<id name="id" type="long" column="LABOR_DETAIL_ID">
			<generator class="native" />
		</id>

		<property name="description" type="string" column="DESCRIPTION"
			length="1000" not-null="true" />
		<property name="amount" type="double" column="AMOUNT" not-null="true" />

	</class>
</hibernate-mapping>